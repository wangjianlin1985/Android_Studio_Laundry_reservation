<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.chengxusheji.domain">

	<class name="OrderInfo" table="OrderInfo">

          <!-- 主键订单编号 -->
          <id name="orderId" column="orderId">
            <generator class="native"/>
          </id>

          <!-- 多对一属性: 洗衣套餐 -->
          <many-to-one name="washMealObj"
            column="washMealObj"
            class="WashMeal"
            lazy="false"
            outer-join="true"
            cascade="save-update"
          />

          <!-- 简单整型属性: 预订数量 -->
          <property
            name="orderCount"
            column="orderCount"
            type="int"

          />

          <!-- 多对一属性: 下单用户 -->
          <many-to-one name="userObj"
            column="userObj"
            class="UserInfo"
            lazy="false"
            outer-join="true"
            cascade="save-update"
          />

          <!-- 简单字符串属性: 联系电话 -->
          <property
            name="telephone"
            column="telephone"
            type="string"
            length="20"
          />

          <!-- 简单字符串属性: 下单时间 -->
          <property
            name="orderTime"
            column="orderTime"
            type="string"
            length="20"
          />

          <!-- 多对一属性: 订单状态 -->
          <many-to-one name="orderState"
            column="orderState"
            class="OrderState"
            lazy="false"
            outer-join="true"
            cascade="save-update"
          />

          <!-- 简单字符串属性: 备注信息 -->
          <property
            name="memo"
            column="memo"
            type="string"
            length="2000"
          />


		 
		
	</class>
	
</hibernate-mapping>
 
